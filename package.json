{
  "name": "fluid-clamp",
  "version": "1.0.2",
  "description": "PostCSS plugin for fluid typography with first-class Tailwind CSS support",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "jest --config jest.config.js",
    "lint": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "tailwindcss",
    "fluid-typography",
    "fluid-type",
    "fluid-spacing",
    "clamp",
    "viewport-units",
    "responsive-typography",
    "css-clamp",
    "fluid-design",
    "responsive-design",
    "css-variables",
    "css-custom-properties",
    "tailwind-plugin"
  ],
  "author": {
    "name": "Rajan Shrestha",
    "url": "https://github.com/razaanstha"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/razaanstha/fluid-clamp"
  },
  "bugs": {
    "url": "https://github.com/razaanstha/fluid-clamp/issues"
  },
  "homepage": "https://github.com/razaanstha/fluid-clamp#readme",
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "dependencies": {
    "postcss-value-parser": "^4.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}